\begin_inset Text

\begin_layout Plain Layout

\size scriptsize
\begin_inset Formula $f(x)=x_{1}^{2}+2x_{2}^{2}-\frac{3}{10}\cos\left(3\pi x_{1}\right)-\frac{4}{10}\cos\left(4\pi x_{2}\right)+\frac{7}{10}$
\end_inset


\end_layout

