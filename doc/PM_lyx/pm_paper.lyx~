\series default
 
\begin_inset Formula $f\left(x_{i}\right)\le f\left(p_{i}\right)$
\end_inset

 then 
\begin_inset Formula $p_{i}=x_{i}$
\end_inset


\end_layout

\end_deeper
\begin_layout Enumerate

\series bold
End
\series default
 
\series bold
For
\end_layout

\begin_layout Enumerate

\series bold
Set
\series default
 
\begin_inset Formula $p_{\mbox{best}}=\arg\min_{i\in1..m}f\left(x_{i}\right)$
\end_inset


\end_layout

\begin_layout Enumerate

\series bold
Set
\series default
 
\begin_inset Formula $\mbox{iter}=\mbox{iter}+1$
\end_inset

.
\end_layout

