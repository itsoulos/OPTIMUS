\begin_layout Subsection
Linear ode equations
\end_layout

\begin_layout Subsubsection*
ODE1
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y'=\frac{2t-y}{t}
\]

\end_inset

with 
\begin_inset Formula $t\in[1,2].$
\end_inset

 The initial condition is 
\begin_inset Formula $y(1)=3$
\end_inset

.
 The solution is 
\begin_inset Formula $y(t)=t+\frac{2}{t}$
\end_inset


\end_layout

\begin_layout Subsubsection*
ODE2
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y'=\frac{1-y\cos(t)}{\sin(t)}
\]

\end_inset

with 
\begin_inset Formula $t\in[1,2].$
\end_inset

 The initial condition is 
\begin_inset Formula $y(1)=\frac{3}{\sin(1)}$
\end_inset

.
 The solution is 
\begin_inset Formula $y(t)=\frac{t+2}{\sin(t)}$
\end_inset


\end_layout

\begin_layout Subsubsection*
ODE3
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y''=6y'-9y
\]

\end_inset

with 
\begin_inset Formula $t\in[0,1]$
\end_inset

.
 The initial conditions are 
\begin_inset Formula $y(0)=0,\ y'(0)=2$
\end_inset

.
 The solution is 
\begin_inset Formula $y(t)=2t\exp(3t)$
\end_inset


\end_layout

\begin_layout Subsubsection*
ODE4
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y''=-\frac{1}{5}y'-y-\frac{1}{5}\exp\left(-\frac{t}{5}\right)\cos(t)
\]

\end_inset

with 
\begin_inset Formula $t\in[0,1]$
\end_inset

.
 The initial conditions are 
\begin_inset Formula $y(0)=0,\ y(1)=\frac{\sin(0.1)}{\exp(0.2)}.$
\end_inset

 The solution is 
\begin_inset Formula $y(t)=\exp\left(-\frac{t}{5}\right)\sin(t)$
\end_inset


\end_layout

\begin_layout Subsubsection*
ODE5
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y''=-100y
\]

\end_inset

with 
\begin_inset Formula $t\in[0,1]$
\end_inset

.
 The initial conditions are 
\begin_inset Formula $y(0)=0,\ y'(0)=10$
\end_inset

.
 The solution is given by
\begin_inset Formula 
\[
y(t)=\sin(10t)
\]

\end_inset


\end_layout

