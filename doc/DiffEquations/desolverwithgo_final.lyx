\begin_layout Subsection
Systems of ode cases 
\end_layout

\begin_layout Subsubsection*
SODE1 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y'_{1} & = & \cos(t)+y_{1}^{2}+y_{2}-\left(t^{2}+\sin^{2}(t)\right)\\
y'_{2} & = & 2t-t^{2}\sin(t)+y_{1}y_{2}
\end{eqnarray*}

\end_inset

with 
\begin_inset Formula $y_{1}(0)=0,\ y_{2}(0)=0,\ t\in[0,1]$
\end_inset

.
 The analytical solutions are given by 
\begin_inset Formula $y_{1}(t)=\sin(t),\ y_{2}(t)=t^{2}$
\end_inset

.
\end_layout

\begin_layout Subsubsection*
SODE2 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y'_{1} & = & \frac{\cos(t)-\sin(t)}{y_{2}}\\
y'_{2} & = & y_{1}y_{2}+\exp(t)-\sin(t)
\end{eqnarray*}

\end_inset

with 
\begin_inset Formula $t\in[0,1]$
\end_inset

 and the initial conditions are 
\begin_inset Formula $y_{1}(0)=0,\ y_{2}(0)=1$
\end_inset

.
 The exact solutions are: 
\begin_inset Formula $y_{1}(x)=\frac{\sin(t)}{\exp(t)},\ y_{2}=\exp(t)$
\end_inset


\end_layout

\begin_layout Subsubsection*
SODE3 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y'_{1} & = & \cos(t)\\
y'_{2} & = & -y_{1}\\
y'_{3} & = & y_{2}\\
y'_{4} & = & -y_{3}\\
y'_{5} & = & y_{4}
\end{eqnarray*}

\end_inset

with 
\begin_inset Formula $y_{1}(0)=0,\ y_{2}(0)=1,\ y_{3}(0)=0,\ y_{4}(0)=1,\ y_{5}(0)=0,\ t\in[0,1]$
\end_inset

 and the solutions 
\begin_inset Formula $y_{1}(t)=\sin(t),\ y_{2}(t)=\cos(t),\ y_{3}(t)=\sin(t),\ y_{4}(t)=\cos(t),\ y_{5}(t)=\sin(t)$
\end_inset

.
\end_layout

\begin_layout Subsubsection*
SODE4
\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{eqnarray*}
y'_{1} & = & -\frac{1}{y_{2}}\sin\left(\exp(t)\right)\\
y'_{2} & = & -y_{2}
\end{eqnarray*}

\end_inset

with 
\begin_inset Formula $y_{1}(0)=\cos(1.0),\ y_{2}(0)=1.0,\ t\in[0,1]$
