\[
y'=\frac{1}{2y}
\]

\end_inset

with 
\begin_inset Formula $t\in[1,4]$
\end_inset

 and the initial condition 
\begin_inset Formula $y(1)=1$
\end_inset

.
 The solution is given by 
\begin_inset Formula $y(t)=\sqrt{t}$
\end_inset


\end_layout

\begin_layout Subsubsection*
NLODE2 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
(y')^{2}+\log(y)-\cos^{2}(t)-2\cos(t)-1-\log(t+\sin(t))=0
\]

\end_inset

with 
\begin_inset Formula $t\in[1,2]$
\end_inset

 and the initial condition 
\begin_inset Formula $y(1)=1+\sin(1)$
\end_inset

.
 The solution is given by 
\begin_inset Formula $y(t)=t+\sin(t)$
\end_inset


\end_layout

\begin_layout Subsubsection*
NLODE3 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
y''y'=-\frac{4}{t^{3}}
\]

\end_inset

with 
\begin_inset Formula $t\in[1,2]$
\end_inset

 and the initial conditions are 
\begin_inset Formula $y(1)=0,\ y(2)=\log(4)$
\end_inset

.
 The solution is 
\begin_inset Formula $y(t)=\log\left(t^{2}\right)$
\end_inset


\end_layout

\begin_layout Subsubsection*
NLODE4 
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
t^{2}y''+\left(ty'\right)^{2}+\frac{1}{\log(t)}=0
\]

\end_inset

with 
\begin_inset Formula $t\in[e,2e]$
\end_inset

 and the initial conditions are 
\begin_inset Formula $y(e)=0,\ y'(e)=\frac{1}{e}$
\end_inset

.
 The solution is 
\begin_inset Formula $y(t)=\log(\log(t))$
\end_inset


\end_layout

